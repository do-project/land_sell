{
  "ID": "do_RichLabel1",
  "IsContainer": "0",
  "Type": "UI",
  "Name": "富文本标签框",
  "Version": "Internal",
  "Description": "富文本签框，设置字体高亮颜色、字体样式",
  "Icon": "https://deviceone.blob.core.chinacloudapi.cn/store/D/20160121/do_RichLabel1/富文本.jpg",
  "IOS": {
    "Support": 1,
    "MinVersion": "7"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "14"
  },
  "Windows": {
    "Support": 0,
    "MinVersion": ""
  },
  "WindowsPhone": {
    "Support": 0,
    "MinVersion": ""
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [
    {
      "ID": "fontColor",
      "Name": "字体颜色",
      "Type": "String",
      "DefaultValue": "000000FF",
      "EditType": "1",
      "Group": "",
      "Description": "设置字体显示颜色，值格式为：8位16进制字符，前6位是RGB颜色值，后两位是透明度（Alpha），例如：000000FF",
      "Design": {
        "Type": "Color",
        "Data": []
      }
    },
    {
      "ID": "fontSize",
      "Name": "字体大小",
      "Type": "Number",
      "DefaultValue": "17",
      "EditType": "1",
      "Group": "",
      "Description": "",
      "Design": {
        "Type": "UInteger",
        "Data": []
      }
    },
    {
      "ID": "fontStyle",
      "Name": "字体风格",
      "Type": "String",
      "DefaultValue": "normal",
      "EditType": "1",
      "Group": "",
      "Description": "包含4种类型：normal：常规bold：粗体italic：斜体bold_italic：粗斜体",
      "Design": {
        "Type": "List",
        "Data": [
          {
            "id": "normal",
            "value": "常规"
          },
          {
            "id": "bold",
            "value": "粗体"
          },
          {
            "id": "italic",
            "value": "斜体"
          },
          {
            "id": "bold_italic",
            "value": "粗斜体"
          }
        ]
      }
    },
    {
      "ID": "maxHeight",
      "Name": "最大高度",
      "Type": "Number",
      "DefaultValue": "",
      "EditType": "0",
      "Group": "",
      "Description": "label的height为\"－1\"的时候，label会根据text内容自动适配变高，但是不会高于maxHeight",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "maxLines",
      "Name": "最多行数",
      "Type": "Number",
      "DefaultValue": "1",
      "EditType": "0",
      "Group": "",
      "Description": "最大行数，缺省是1，如果为小于或等于0表示不限行数，说明：设置文本内容显示最大行数，如显示内容超过了最大行值则结尾用省略号\"...\"表示；iOS平台不支持",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "maxWidth",
      "Name": "最大宽度",
      "Type": "Number",
      "DefaultValue": "",
      "EditType": "0",
      "Group": "",
      "Description": "设置文本框显示最大宽度值，只有在设置\"Width\"属性值为\"-1\"时有效，否则不起作用;",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "span",
      "Name": "设置区域内显示富文本字体样式",
      "Type": "String",
      "DefaultValue": "",
      "EditType": "1",
      "Group": "",
      "Description": "值格式为：\r\n\t\t[\r\n\t\t\t{\r\n\t\t\t\t\"strMatch\": \"a\",//匹配字符串\r\n\t\t\t\t\"substring\": \"0,10\",//截取字符串，前一位为起点，后一位为终点\r\n\t\t\t\t\"spanStyle\": \"{fontColor:'FF3030FF',fontStyle:'bold'}\",//区域内字体显示样式\r\n\t\t\t\t\"allowTouch\": false,//是否允许点击事件\r\n\t\t\t\t\"tag\": \"自定义数据\"\r\n\t\t\t},\r\n\t\t\t{},\r\n\t\t\t...\r\n\t\t]",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "text",
      "Name": "文本",
      "Type": "String",
      "DefaultValue": "",
      "EditType": "1",
      "Group": "",
      "Description": "文本显示内容",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "textAlign",
      "Name": "文本对齐方式",
      "Type": "String",
      "DefaultValue": "left",
      "EditType": "0",
      "Group": "",
      "Description": "对齐方式为以下3种：left 左对齐（默认）；center 居中；right 右对齐。",
      "Design": {
        "Type": "List",
        "Data": [
          {
            "id": "left",
            "value": "左对齐"
          },
          {
            "id": "center",
            "value": "居中"
          },
          {
            "id": "right",
            "value": "右对齐"
          }
        ]
      }
    },
    {
      "ID": "textFlag",
      "Name": "字体标示",
      "Type": "String",
      "DefaultValue": "normal",
      "EditType": "0",
      "Group": "",
      "Description": "包含3种类型：normal：常规underline ：下划线strikethrough ：删除线",
      "Design": {
        "Type": "List",
        "Data": [
          {
            "id": "normal",
            "value": "常规"
          },
          {
            "id": "underline",
            "value": "下划线"
          },
          {
            "id": "strikethrough",
            "value": "删除线"
          }
        ]
      }
    }
  ],
  "Events": [
    {
      "ID": "touch",
      "Description": "匹配内容点击事件",
      "ReturnType": "Node",
      "ReturnDesc": "返回匹配到的内容content和tag数据"
    }
  ],
  "syncMethodes": [],
  "asyncMethodes": []
}